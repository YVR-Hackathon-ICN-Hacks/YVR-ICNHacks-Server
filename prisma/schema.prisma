generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @map("_id")
  email           String  @unique
  verified_email  Boolean? @default(false)
  name            String
  given_name      String
  family_name     String
  picture         String
  locale          String
  hd              String?
}