generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @map("_id")
  email           String  @unique
  verified_email  Boolean? @default(false)
  name            String
  given_name      String
  family_name     String
  picture         String
  locale          String
  hd              String?
}

model Data {
  id          String  @id @map("_id")
  area_id     String
  timestamp   DateTime
  temperature Float?
  air_flow    Float?
  co2         Float?
}